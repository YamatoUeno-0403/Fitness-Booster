    <%= form_with model:post ,local: true do |f| %>
        <div class="form-group">
            <%= f.label :supplement %>
            <%= f.text_field :supplement, class: 'form-control book_title' %>
        </div>
        <div class="form-group">
            <%= f.label :content %>
            <%= f.text_area :content, class: 'form-control book_body' %>
        </div>
        
        <div class="form-group">
            <%= f.submit "投稿する", class: 'btn btn-primary' %></div>
    <% end %>